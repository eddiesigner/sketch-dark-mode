{
  "$schema": "https://raw.githubusercontent.com/sketch-hq/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "author": "Eduardo GÃ³mez",
  "authorEmail": "this.eduardo@gmail.com",
  "bundleVersion": 1,
  "icon": "icon.png",
  "commands": [
    {
      "name": "Manage Palette...",
      "identifier": "sketch-dark-mode.create-palette-identifier",
      "script": "./create-palette.js",
      "shortcut": "shift cmd p",
      "handlers": {
        "run": "onRun",
        "actions": {
          "Shutdown": "onShutdown"
        }
      }
    },
    {
      "name": "Generate Dark Mode for Selected Page",
      "identifier": "sketch-dark-mode.create-dark-page-identifier",
      "script": "./create-dark-page.js",
      "shortcut": "shift cmd d"
    },
    {
      "name": "Generate Dark Mode for Selected Artboards",
      "identifier": "sketch-dark-mode.create-dark-artboard-identifier",
      "script": "./create-dark-artboard.js",
      "shortcut": "shift cmd b"
    },
    {
      "name": "How to Use this Plugin...",
      "identifier": "sketch-dark-mode.how-it-works-identifier",
      "script": "./how-it-works.js"
    }
  ],
  "menu": {
    "title": "ðŸŒ“ Sketch Dark Mode",
    "items": [
      "sketch-dark-mode.create-palette-identifier",
      "sketch-dark-mode.create-dark-page-identifier",
      "sketch-dark-mode.create-dark-artboard-identifier",
      "sketch-dark-mode.how-it-works-identifier"
    ]
  },
  "compatibleVersion": "54",
  "homepage": "https://sketch-dark-mode.eduardogomez.io",
  "identifier": "io.eduardogomez.sketch-dark-mode",
  "name": "Sketch Dark Mode",
  "description": "Generate a dark mode version of any document, the right way."
}
